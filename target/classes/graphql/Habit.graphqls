type Query {
  getAllHabits: [HabitDTO!]!
  getHabitById(id: ID!): HabitDTO
}

type Mutation {
  createHabit(input: HabitInput!): HabitDTO
  deleteHabit(id: ID!): Boolean
}

type HabitDTO {
  id: ID!
  name: String!
  category: String
  description: String
}

input HabitInput {
  id: ID
  name: String!
  category: String
  description: String
}