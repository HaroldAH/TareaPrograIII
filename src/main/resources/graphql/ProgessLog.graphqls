type Query {
  getAllProgressLogs: [ProgressLogDTO!]!
  getProgressLogById(id: ID!): ProgressLogDTO
}

type Mutation {
  createProgressLog(input: ProgressLogInput!): ProgressLogDTO
  deleteProgressLog(id: ID!): Boolean
}

type ProgressLogDTO {
  id: ID!
  userId: ID!
  routineId: ID!
  date: String
}

input ProgressLogInput {
  id: ID
  userId: ID!
  routineId: ID!
  date: String
}